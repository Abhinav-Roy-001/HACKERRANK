#include <bits/stdc++.h>
using namespace std;

vector<int> jimOrders(vector<vector<int>> orders) {
    int n = orders.size();
    vector<pair<int, int>> serveTimes;

    for (int i = 0; i < n; i++) {
        int serveTime = orders[i][0] + orders[i][1]; 
        serveTimes.push_back({serveTime, i + 1});    
    }
    sort(serveTimes.begin(), serveTimes.end());
    vector<int> result;
    for (auto &p : serveTimes) {
        result.push_back(p.second);
    }
    return result;
}

int main() {
    int n;
    cin >> n;
    vector<vector<int>> orders(n, vector<int>(2));
    for (int i = 0; i < n; i++) {
        cin >> orders[i][0] >> orders[i][1];
    }

    vector<int> result = jimOrders(orders);
    for (int i = 0; i < result.size(); i++) {
        cout << result[i] << (i + 1 == result.size() ? "\n" : " ");
    }
    return 0;
