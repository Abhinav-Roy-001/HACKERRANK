#include <bits/stdc++.h>
using namespace std;

string organizingContainers(vector<vector<int>> container) {
    int n = container.size();
    vector<long long> containerSum(n, 0), ballTypeSum(n, 0);

    for (int i = 0; i < n; i++) {
        for (int j = 0; j < n; j++) {
            containerSum[i] += container[i][j];
            ballTypeSum[j] += container[i][j];
        }
    }

    sort(containerSum.begin(), containerSum.end());
    sort(ballTypeSum.begin(), ballTypeSum.end());

    return (containerSum == ballTypeSum) ? "Possible" : "Impossible";
}

int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);

    int q;
    cin >> q;
    while (q--) {
        int n;
        cin >> n;
        vector<vector<int>> container(n, vector<int>(n));
        for (int i = 0; i < n; i++) {
            for (int j = 0; j < n; j++) {
                cin >> container[i][j];
            }
        }
        cout << organizingContainers(container) << "\n";
    }
    return 0;
}
