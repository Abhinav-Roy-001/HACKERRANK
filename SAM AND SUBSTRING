#include <bits/stdc++.h>
using namespace std;
int main() {
    string n;
    cin >> n;
    long long mod = 1000000007, res = 0, f = 1;
    for (int i = n.size() - 1; i >= 0; i--) {
        res = (res + (n[i] - '0') * f * (i + 1)) % mod;
        f = (f * 10 + 1) % mod;
    }
    cout << res % mod;
}
