#include <bits/stdc++.h>
using namespace std;

int main() {
    int n; cin >> n;
    unordered_map<long long,long long> mp;
    while (n--) {
        int m,d; cin >> m >> d;
        if (m <= 1) continue;
        string s = to_string(d);
        bool ok = true; long long val = 0;
        for (char c : s) {
            int x = c - '0';
            if (x >= m) { ok = false; break; }
            val = val * m + x;
        }
        if (ok) mp[val]++;
    }
    long long ans = 0;
    for (auto &p : mp) ans += p.second * (p.second - 1) / 2;
    cout << ans;
}
