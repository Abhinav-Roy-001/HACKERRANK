#include <bits/stdc++.h>
using namespace std;

int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);
    
    int n;
    if (!(cin >> n)) return 0;
    unordered_map<long long, long long> freq;
    freq.reserve(n*2);
    
    for (int i = 0; i < n; ++i) {
        int m, d;
        cin >> m >> d;
        if (m <= 1) continue;
        
        string s = to_string(d);
        bool valid = true;
        for (char ch : s) {
            int digit = ch - '0';
            if (digit >= m) { valid = false; break; }
        }
        if (!valid) continue;
        
        long long val = 0;
        for (char ch : s) {
            val = val * m + (ch - '0');
        }
        freq[val]++;
    }
    long long ans = 0;
    for (auto &p : freq) {
        long long f = p.second;
        ans += f * (f - 1) / 2;   
    }
    
    cout << ans << '\n';
    return 0;
}
